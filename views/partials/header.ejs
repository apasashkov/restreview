<!doctype html>
<html>
<head>
  <title></title>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">  
  <link href="/stylesheets/main.css" rel="stylesheet">  </head>
  <link rel="stylesheet" href="/stylesheets/css/font-awesome.min.css">

<body>
<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/restaraunts">RestReview</a>
    </div>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav navbar-right">
        <% if(!currentUser){ %>
          <li> <a href="/login">login</a></li>
          <li> <a href="/register">sign up</a></li>


          <% } else  { %>
          <li> <a href="#">Signed in as <strong><%=currentUser.username%></strong></a></li>
          <li> <a href="/logout">Logout</a></li>
          <% } %>

      </ul>
    </div>
  </div>
</nav>

<div class="container">
  <% if(error && error.length>0){ %>
    <div class="alert alert-danger" role="alert">
      <%=error%>
    </div>
  <%}%>


  <% if(success && success.length>0){ %>
  <div class="alert alert-success" role="alert">
    <%=success%>
  </div>
  <%}%>
</div>


